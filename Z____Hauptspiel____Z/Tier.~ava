

public class Tier {
  
  // Anfang Attribute
  public String Name;
  public String Eigenschaften;
  public void Bild;
  // Ende Attribute
  
  // Anfang Methoden
  public String getName() {
    return Name;
  }

  public String getEigenschaften() {
    return Eigenschaften;
  }
  public void getBild() {
    return Bild;
  }

  // Ende Methoden
} // end of Tier

